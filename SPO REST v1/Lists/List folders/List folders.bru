meta {
  name: List folders
  type: http
  seq: 1
}

get {
  url: https://{{tenantPrefix}}.sharepoint.com{{siteRelativePath}}/_api/web/Lists/getByTitle('Customers')/Items?$select=*,FileDirRef&$filter=FSObjType eq 1
  body: none
  auth: inherit
}

params:query {
  $select: *,FileDirRef
  $filter: FSObjType eq 1
  ~$filter: FSObjType eq 1 and Title eq 'Yvand'
}

headers {
  Accept: application/json
  Content-Type: application/json
}
