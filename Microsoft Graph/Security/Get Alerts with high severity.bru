meta {
  name: Get Alerts with high severity
  type: http
  seq: 2
}

get {
  url: https://graph.microsoft.com/v1.0/security/alerts?$filter=Severity eq 'High'&$top=5
  body: none
  auth: bearer
}

params:query {
  $filter: Severity eq 'High'
  $top: 5
}

headers {
  SdkVersion: postman-graph/v1.0
}

auth:bearer {
  token: {{UserAccessToken}}
}

